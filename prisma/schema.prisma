

datasource db {
  url      = env("DATABASE_URL")
  provider = "postgresql"
}

generator client {
  provider = "prisma-client-js"
}

model User {
  id        Int      @id @default(autoincrement())
  createdAt DateTime @default(now())
  email     String   @unique
  name      String?
}

model Playlist {
  id        String   @id @default(cuid())
  createdAt DateTime @default(now())
  title     String
  songs     Json
}

